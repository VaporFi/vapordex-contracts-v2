{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "compile": {
      "dependsOn": ["^compile"],
      "outputs": ["artifacts/**", "cache/**", "typechain/**"]
